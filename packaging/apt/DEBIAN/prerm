#!/bin/bash
set -e

# Remove wrapper script and helper functions
rm -f /usr/local/bin/awsr
rm -f /usr/local/share/aws-assume-role/shell-helpers.sh
rmdir /usr/local/share/aws-assume-role 2>/dev/null || true

echo "AWS Assume Role CLI has been removed!"
echo "Note: Your role configurations in ~/.aws-assume-role/ are preserved."

exit 0 